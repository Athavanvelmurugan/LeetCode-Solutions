class Solution {
    public int findNumbers(int[] nums) {
        int count =0;
        for(int i=0; i<nums.length; i++){
            count+=checkeven(nums[i]);
    }
    return count;
    }

    public static int checkeven(int num){
        int  count=0;
        while(num!=0){
        count++;
        num/=10;
        }
        if(count%2==0){
            return 1;
        }
        return 0;
    }
}
